@charset "EUC-KR";
/*이미지 CSS 시작*/
#overlap{
	position:absolute;
	left:0px;
	top:0px;
	width:32px;
	height:77px; 
	background-color:green;
	opacity:0.8;
	z-index:20;
}
#main, #main_map, #main_text{
	border: 2px solid black;
}
#main_tbl{
	width:100%;
	height:100%;
	background-color:black;
	color:white;
}
#mainimg{
	width:53%;
	height:auto;
}
#mainimg img{
	width:1100px;
	height:auto;
}
/*이미지 CSS 끝*/
/*층별 CSS 시작*/
#nthfloor{
	width:100%;
	height:100%;
	text-align:center;
	border: 3px solid white;
}
#nthfloor_td1{
	width:540px;	
}

#nthfloor_td2{
	width:540px;
}

#nthfloor h2{
	color:yellow;
}
#nthfloor td{
	font-weight:bold;
	
}
#nthfloor td:nth-child(2){
	text-align:right;
	width:20%;
	font-size:25px;
	letter-spacing:3px;
}
#nthfloor td:nth-child(3){
	width:15%;
	font-size:25px;
	
}
#nthfloor td:nth-child(4){
	text-align:left;
	width:20%;
	font-size:25px;
	letter-spacing:3px;
	color:#00D04D;
}
#nthfloor #icon{
	width:30%;
	text-align:right;
}
#nthfloor img:nth-child(1){
	width:30px;
	background-color:white;	
	border-radius:6px;
	border:1px solid black;
}
#nthfloor2{
	width:100%;
	height:100%;
	text-align:center;
	border:3px solid white;
}
#nthfloor2 h2{
	color:yellow;
}
#nthfloor2{
	font-weight:bold;
} 	
#nthfloor2 img:nth-child(1){
	width:30px;
	background-color:white;	
	border-radius:6px;
	border:1px solid black;
}
#nthfloor2 #icon{
	width:30%;
	text-align:right;
}
#nthfloor2 td:nth-child(2){
	width:20%;
	font-size:25px;
}
#nthfloor2 td:nth-child(3){
	width:20%;
	font-size:25px;
}
/*층별 CSS 끝*/
/*side CSS 시작 */
#side_data{
	width:100%;
	height:100%;
}
#top_data{
	height:35%;
}
#bottom_data{
	height:60%;
}
#top_data table{
	width:100%;
	height:100%;
	text-align:center;
}
#top_data #img_td{
	width:60%;
	height:60%;
	overflow:hidden;
}
#top_data img{
	width:100%;
	height:100%;
	object-fit: cover;
}
#top_data table input{
	width:150px;
	background-color:gray;
}
#top_data td:nth-child(1){
	text-align:right;
	
}
#top_data td:nth-child(2){
	text-align:left;
	
}
#top_data table #top_data_text{
	height:100%;
}
#top_data #top_dataButton{
	text-align:center;
}
#bottom_data table{
	width:100%;
	height:100%;
	text-align:center;
}
#bottom_board{
	width:100%;
	height:610px;
	overflow:auto;
}
#bottom_data_tbl th{
	position: sticky;
    top: 0px;
    background-color:gray;
}
#bottom_data_button{
	background-color:gray;
	width:150px;
	height:45px;
	margin:45px;
	font-size:20px;
	font-weight:bold;
}
/*side CSS 끝 */
