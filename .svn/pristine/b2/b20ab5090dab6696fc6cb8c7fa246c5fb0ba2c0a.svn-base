<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<table border="1">
	<tr>
		<td id="img_td">
			<img src="../img/car1.jpg">
		</td>
		<td	rowspan="2">
